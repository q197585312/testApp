<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:orientation="vertical">

    <include
        layout="@layout/toolbar_layout"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#60000000"
            android:paddingLeft="20dp"
            android:paddingRight="20dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/gd_img_open"
                    android:layout_width="30dp"
                    android:layout_height="27dp"
                    android:src="@mipmap/gd_menu"
                    android:visibility="visible"></ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Menu"
                    android:textColor="@color/white"></TextView>
            </LinearLayout>

            <ImageView
                android:id="@+id/gd_img_gif"
                android:layout_width="190dp"
                android:layout_height="70dp"
                android:layout_gravity="center"
                android:scaleType="fitXY"></ImageView>

            <LinearLayout
                android:id="@+id/ll_ahl_login"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@mipmap/ahl_user"
                    android:visibility="visible"></ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gd_login"
                    android:textColor="@color/white"></TextView>
            </LinearLayout>
        </FrameLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">

            <LinearLayout
                android:id="@+id/gd__ll_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/gd_img_login_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="40dp"
                        android:src="@mipmap/gd_app_logo" />

                    <ImageView
                        android:id="@+id/gd_img_login_title_main"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginRight="20dp"
                        android:background="@mipmap/gd_title_logo"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/gd_img_login_title_main_sbocasino77"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="60dp"
                        android:layout_marginRight="25dp"
                        android:background="@mipmap/gd_title_logo"
                        android:visibility="gone" />

                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@mipmap/top_bg"
                    android:orientation="vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="45dp"
                            android:layout_height="39dp"
                            android:layout_gravity="center"
                            android:background="@mipmap/mq_logo"></ImageView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="6dp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <gaming178.com.casinogame.Control.AutoScrollTextView
                                android:id="@+id/gd__hall_game_bottom_prompt_tv"
                                android:layout_width="match_parent"
                                android:layout_height="22dp"
                                android:gravity="center_vertical" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginTop="3dp"
                                android:background="@color/white"></View>
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"></View>

                    <com.zhpan.bannerview.BannerViewPager
                        android:id="@+id/banner_view"
                        android:layout_width="match_parent"
                        android:layout_height="160dp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"></View>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/gd_yellow_hint_bg"
                            android:paddingLeft="25dp"
                            android:paddingTop="10dp"
                            android:paddingRight="25dp"
                            android:paddingBottom="10dp"
                            android:text="KONTAK RESMI"
                            android:textColor="#F9D328"
                            android:textSize="25sp"
                            android:textStyle="bold"></TextView>

                        <LinearLayout
                            android:id="@+id/ll_ahl_content"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@mipmap/contact_right"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/ll_wa"
                                android:layout_width="wrap_content"
                                android:layout_height="105dp"
                                android:background="@drawable/shape_contact_item_bg"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="6dp">

                                <ImageView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@mipmap/whatsapp"></ImageView>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginBottom="5dp"
                                    android:text="Whatsapp"
                                    android:textColor="#55CD6C"></TextView>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="+85595306890 "
                                    android:textColor="@color/black"></TextView>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_tg"
                                android:layout_width="wrap_content"
                                android:layout_height="105dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:background="@drawable/shape_contact_item_bg"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="6dp">

                                <ImageView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@mipmap/telegram"></ImageView>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginBottom="5dp"
                                    android:text="Telegram"
                                    android:textColor="#1D9ED4"></TextView>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=" @ahlicasino "
                                    android:textColor="@color/black"></TextView>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_liveChat"
                                android:layout_width="wrap_content"
                                android:layout_height="105dp"
                                android:background="@drawable/shape_contact_item_bg"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="6dp">

                                <ImageView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:background="@mipmap/livechat"></ImageView>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:text="Livechat"
                                    android:textColor="#FD5401"></TextView>

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"></View>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_lg_and_remember"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/gd__ll_choose_language"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/gd_rectangle_white_radius10"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:id="@+id/gd__iv_language_flag"
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:src="@mipmap/gd_flag_british_bg" />

                        <ImageView
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:layout_marginLeft="30dp"
                            android:src="@mipmap/white_bottom" />


                    </LinearLayout>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"></View>

                    <LinearLayout
                        android:id="@+id/ll_remember_me"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/remember_me"
                            android:textColor="@color/white"
                            android:textSize="16sp"></TextView>

                        <CheckBox
                            android:id="@+id/cb_remember_me"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:background="@drawable/checkbox_login_style"
                            android:button="@null"
                            android:clickable="false"></CheckBox>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="40dp"
                    android:background="@mipmap/bottom_bg"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="20dp"></View>

                            <ImageView
                                android:id="@+id/img_animation1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@mipmap/item1"></ImageView>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/shape_animation_item_bg"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="5dp"
                                android:text="Live Casino"
                                android:textAllCaps="true"
                                android:textColor="@color/black"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="20dp"></View>

                            <ImageView
                                android:id="@+id/img_animation2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@mipmap/item2"></ImageView>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/shape_animation_item_bg"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="5dp"
                                android:text="Slot Online"
                                android:textAllCaps="true"
                                android:textColor="@color/black"></TextView>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="20dp"></View>

                            <ImageView
                                android:id="@+id/img_animation3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@mipmap/item3"></ImageView>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/shape_animation_item_bg"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="5dp"
                                android:text="Sportbook"
                                android:textAllCaps="true"
                                android:textColor="@color/black"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="20dp"></View>

                            <ImageView
                                android:id="@+id/img_animation4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@mipmap/item5"></ImageView>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:background="@drawable/shape_animation_item_bg"
                                android:paddingLeft="20dp"
                                android:paddingTop="5dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="5dp"
                                android:text="Fishing Game"
                                android:textAllCaps="true"
                                android:textColor="@color/black"></TextView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_login_prent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/gd_login_bg"
        android:orientation="vertical"
        android:visibility="gone">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginLeft="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/gd_login"
                android:textColor="@color/black"
                android:textSize="25sp"></TextView>

            <LinearLayout
                android:id="@+id/ll_login_prent_exit"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="x"
                    android:textColor="#808080"
                    android:textSize="30sp"></TextView>
            </LinearLayout>
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#DEE2E6"></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="9dp"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_user_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:drawablePadding="6dp"
                    android:gravity="center"
                    android:text="@string/input_username"
                    android:textColor="@color/black"
                    android:textSize="16sp"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:drawablePadding="6dp"
                    android:gravity="center"
                    android:text=":"
                    android:textColor="@color/black"
                    android:textSize="16sp"></TextView>
            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="53dp">

                <FrameLayout
                    android:id="@+id/fl_name_out_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ahl_input_out_bg"
                    android:visibility="gone"></FrameLayout>

                <EditText
                    android:id="@+id/gd__login_username_edt"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/ahl_input_bg"
                    android:hint="@string/input_username"
                    android:paddingLeft="15dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textColorHint="#888C8E"
                    android:textCursorDrawable="@null"
                    android:textSize="16sp" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="15dp"
            android:layout_marginRight="9dp"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_password_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:drawablePadding="6dp"
                    android:gravity="center"
                    android:text="@string/input_password"
                    android:textColor="@color/black"
                    android:textSize="16sp"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:drawablePadding="6dp"
                    android:gravity="center"
                    android:text=":"
                    android:textColor="@color/black"
                    android:textSize="16sp"></TextView>
            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="53dp">

                <FrameLayout
                    android:id="@+id/fl_password_out_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ahl_input_out_bg"
                    android:visibility="gone"></FrameLayout>

                <EditText
                    android:id="@+id/gd__login_password_edt"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/ahl_input_bg"
                    android:hint="@string/input_password"
                    android:inputType="textPassword"
                    android:paddingLeft="15dp"
                    android:textColor="@color/black"
                    android:textColorHint="#888C8E"
                    android:textCursorDrawable="@null"
                    android:textSize="16sp" />
            </FrameLayout>


        </LinearLayout>

        <Button
            android:id="@+id/gd__login_login_btn"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/gd_login_button_bg"
            android:gravity="center"
            android:onClick="clickLogin"
            android:text="@string/gd_login"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="30dp"
            android:background="#DEE2E6"></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingTop="20dp"
            android:paddingBottom="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="belum punya akun?"
                android:textColor="@color/black"></TextView>

            <TextView
                android:id="@+id/gd__tv_register"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:background="@drawable/gd_register_button_bg"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingTop="15dp"
                android:paddingRight="5dp"
                android:paddingBottom="15dp"
                android:text="DAFTAR DISINI"
                android:textAllCaps="true"
                android:textColor="@color/black" />
        </LinearLayout>

    </LinearLayout>


    <RelativeLayout
        android:id="@+id/gd_rl_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#85000000"
        android:visibility="gone">

        <WebView
            android:id="@+id/gd_login_webview"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_centerInParent="true"></WebView>

        <ImageView
            android:id="@+id/gd_img_exit"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_below="@+id/gd_login_webview"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:background="@mipmap/gd_login_exit"></ImageView>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_sbocasino77"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="55dp"
        android:background="#E6EDFF"
        android:orientation="horizontal"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:paddingBottom="8dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:src="@mipmap/sbocasino77_sports"></ImageView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:text="SPORT"
                android:textColor="#2E3C83"
                android:textSize="12sp"></TextView>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#2E3C83"></View>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:paddingBottom="8dp">

            <ImageView
                android:layout_width="match_parent"
