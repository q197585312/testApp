<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/gd__ll_baccarat_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/gd_way_bg"
        android:padding="2.5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:background="@drawable/gd_rectangle_brown_top_corner5"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/gd_rectangle_brown_top_corner5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/gd__tv_baccarat_table_name"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:padding="2dp"
                        android:singleLine="true"
                        android:text="BACCARAT01"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/gd__text_shoe_game_number"
                        style="@style/text_wrap_black12"
                        android:layout_width="70dp"
                        android:layout_marginLeft="5dp"
                        android:text="0-0"
                        android:textColor="@color/white" />

                    <LinearLayout
                        android:layout_width="38dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_b_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:id="@+id/gd__tv_banker"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/banker1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_banker"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="38dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_p_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:id="@+id/gd__tv_player"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/player1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_player"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="38dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_tie_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:id="@+id/gd__tv_tie"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/tie1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_tie"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/gd__ll_bp"
                        android:layout_width="38dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_bp_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:id="@+id/gd__tv_banker_pair"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/BP1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_bp"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/gd__ll_pp"
                        android:layout_width="38dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_pp_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:id="@+id/gd__tv_player_pair"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/PP1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_pp"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="45dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/gd_rectangle_total_shape"
                        android:orientation="horizontal"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="@string/total_m"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__text_total"
                            style="@style/text_wrap_black12"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/white" />
                </LinearLayout>

                <TextView
                    android:id="@+id/gd__baccarat_status_tv"
                    android:layout_width="24dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/gd_rectangle_trans_stroke_white"
                    android:gravity="center"
                    android:minWidth="100dp"
                    android:padding="1dp"
                    android:singleLine="true"
                    android:text="0"
                    android:textColor="@color/yellow_brown_a3721a"
                    android:textSize="14sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_dragon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone"></LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="@color/white" />

                <LinearLayout
                    android:id="@+id/gd__baccarat_grid_parent_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/gd__ll_layout_girl"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/gd__baccarat_head_iv"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:scaleType="centerCrop" />

                            <TextView
                                android:id="@+id/gd__tv_baccarat_dealer_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom|center_horizontal"
                                android:gravity="center"
                                android:padding="2dp"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="@color/white"
                                android:textSize="9sp"
                                android:textStyle="bold"
                                android:visibility="visible" />
                        </FrameLayout>


                    </LinearLayout>

                    <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                        android:id="@+id/gd__layout1"
                        android:layout_width="161dp"
                        android:layout_height="wrap_content"
                        android:scrollbars="none">

                        <include layout="@layout/gd_include_baccarat_grid_layout1" />
                    </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="21"
                        android:orientation="vertical">

                        <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                            android:id="@+id/gd__ll_big_road_parent2_landscape"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="false"
                            android:scrollbars="none">

                            <include
                                android:id="@+id/gd__big_way"
                                layout="@layout/gd_include_baccarat_grid_layout2" />
                        </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="-1dp"
                            android:orientation="horizontal">

                            <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                                android:id="@+id/gd__hsv_small_road_1_landscape"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:scrollbars="none">

                                <include layout="@layout/gd_include_baccarat_grid_layout3" />
                            </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                            <View style="@style/line_vertical_grey1" />

                            <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                                android:id="@+id/gd__hsv_small_road_2_landscape"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="1dp"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:scrollbars="none">

                                <include
                                    android:id="@+id/gd__baccarat_gridlayout4"
                                    layout="@layout/gd_include_baccarat_grid_layout3" />
                            </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                            <View style="@style/line_vertical_grey1" />

                            <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                                android:id="@+id/gd__hsv_small_road_3_landscape"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="1dp"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:scrollbars="none">

                                <include
                                    android:id="@+id/gd__baccarat_gridlayout5"
                                    layout="@layout/gd_include_baccarat_grid_layout3" />
                            </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                            <View style="@style/line_vertical_grey1" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="@drawable/gd_rectangle_white_bottom_corner5" />

            </LinearLayout>
        </LinearLayout>

        <include layout="@layout/gd_good_road_bg"></include>

        <include layout="@layout/gd_table_close_bg"></include>

        <include layout="@layout/gd_shuffling" />

        <include
            layout="@layout/gd_popupwindow_limit_select_layout"
            android:visibility="gone" />
    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"></View>

</LinearLayout>