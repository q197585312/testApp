<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/gd__fl_big_road1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="false">

    <gaming178.com.mylibrary.myview.View.GridBackgroundView
        android:id="@+id/gd__baccarat_background_gridlayout1"
        android:layout_width="301dp"
        android:layout_height="121dp"
        android:clickable="false"
        app:dividing_line_spacing="20dp"
        app:line_color="@color/grey_default_bg" />
    <gaming178.com.mylibrary.myview.View.GridBackgroundView
        android:id="@+id/gd__baccarat_background_gridlayout1_big"
        android:layout_width="match_parent"
        android:layout_height="121dp"
        android:visibility="gone"
        android:clickable="false"
        app:dividing_line_spacing="20dp"
        app:line_color="@color/grey_default_bg" />

    <GridLayout
        android:id="@+id/gd__baccarat_gridlayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false"
        android:columnCount="15"
        android:orientation="vertical"
        android:rowCount="6"></GridLayout>
</FrameLayout>