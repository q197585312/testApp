<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="parentClick">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginRight="122px"
        android:layout_marginBottom="186px"
        android:gravity="center"
        android:onClick="clickChangeBetContent"
        android:padding="5dp"
        android:text="BACCARAT"
        android:textColor="#00000000"
        android:textSize="13sp"></TextView>

    <FrameLayout
        android:id="@+id/gd__fl_poker_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false">

        <FrameLayout
            android:id="@+id/gd__fl_poker_bottom_parent"
            android:layout_width="660px"
            android:layout_height="280px"
            android:layout_gravity="bottom|center_horizontal"
            android:clickable="false"
            android:visibility="gone">

            <FrameLayout
                android:layout_width="660px"
                android:layout_height="266px"
                android:layout_gravity="bottom|center_horizontal"
                android:background="@color/trans_black_dark"
                android:clickable="false"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="false"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/gd__v_background_player"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/gd_rectangle_trans_stroke_yellow_corner4_right" />

                    <View
                        android:id="@+id/gd__v_background_banker"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/gd_rectangle_trans_stroke_yellow_corner4_right"
                        android:visibility="gone" />
                </LinearLayout>


            </FrameLayout>

            <TextView
                android:id="@+id/gd__tv_poker_timer"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/gd_oval_black_solid"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/red_background"
                android:textSize="22sp"
                android:textStyle="bold"
                android:visibility="gone" />
        </FrameLayout>
        <!--          android:background="@drawable/oval_blue_solid_no_line"-->
        <!--       android:background="@drawable/oval_red_solid_no_line"-->
        <FrameLayout
            android:id="@+id/gd__ll_poker_parent_a"
            android:layout_width="526px"
            android:layout_height="245px"
            android:layout_gravity="bottom|center_horizontal"
            android:clickable="false"
            android:orientation="horizontal">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:clickable="false"

                android:orientation="vertical"
                android:visibility="visible">


                <ImageView
                    android:id="@+id/gd__iv_poker_player1_a"
                    android:layout_width="58px"
                    android:layout_height="38px"
                    android:layout_marginLeft="18px"
                    android:layout_marginTop="160px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_player2_a"
                    android:layout_width="58px"
                    android:layout_height="38px"
                    android:layout_marginLeft="77px"
                    android:layout_marginTop="160px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />


                <ImageView

                    android:id="@+id/gd__iv_poker_player3_a"
                    android:layout_width="66px"
                    android:layout_height="32px"
                    android:layout_marginLeft="36px"
                    android:layout_marginTop="206px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_player1_a1"
                    android:layout_width="80px"
                    android:layout_height="67px"
                    android:layout_gravity="bottom"
                    android:layout_marginBottom="138px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_player2_a1"
                    android:layout_width="80px"
                    android:layout_height="67px"
                    android:layout_gravity="bottom"
                    android:layout_marginLeft="73px"
                    android:layout_marginBottom="138px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_player3_a1"
                    android:layout_width="91px"
                    android:layout_height="53px"
                    android:layout_gravity="bottom"
                    android:layout_marginLeft="14px"
                    android:layout_marginBottom="72px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="263px"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:clickable="false">


                <ImageView
                    android:id="@+id/gd__iv_poker_banker1_a"
                    android:layout_width="54px"
                    android:layout_height="38px"
                    android:layout_gravity="right"
                    android:layout_marginTop="160px"
                    android:layout_marginRight="90px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_banker2_a"
                    android:layout_width="58px"
                    android:layout_height="38px"
                    android:layout_gravity="right"

                    android:layout_marginTop="160px"
                    android:layout_marginRight="60px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_banker3_a"
                    android:layout_width="66px"
                    android:layout_height="32px"
                    android:layout_gravity="right"
                    android:layout_marginTop="206px"
                    android:layout_marginRight="45px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_banker1_a1"
                    android:layout_width="78px"
                    android:layout_height="67px"
                    android:layout_gravity="bottom|right"
                    android:layout_marginRight="74px"
                    android:layout_marginBottom="138px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/gd__iv_poker_banker2_a1"
                    android:layout_width="78px"
                    android:layout_height="67px"
                    android:layout_gravity="right|bottom"
                    android:layout_marginBottom="138px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />


                <ImageView
                    android:id="@+id/gd__iv_poker_banker3_a1"
                    android:layout_width="91px"
                    android:layout_height="53px"
                    android:layout_gravity="bottom|right"
                    android:layout_marginRight="14px"
                    android:layout_marginBottom="72px"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    android:src="@mipmap/gd_pk_1"
                    android:visibility="gone" />


            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:layout_width="680px"
            android:layout_height="245px"
            android:layout_gravity="bottom|center_horizontal"
            android:clickable="false"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/gd__tv_bottom_panel_left_a"
                android:layout_width="32px"
                android:layout_height="40px"
                android:layout_gravity="left"
                android:layout_marginLeft="50px"
                android:layout_marginTop="70px"
                android:background="@mipmap/gd_poker_bg_small"
                android:gravity="center"
                android:textColor="#0000FF"
                android:textSize="27px"
                android:visibility="gone"
                app:layout_auto_basewidth="height" />

            <TextView
                android:id="@+id/gd__tv_bottom_panel_right_a"
                android:layout_width="32px"
                android:layout_height="40px"
                android:layout_gravity="right"
                android:layout_marginTop="70px"
                android:layout_marginRight="50px"
                android:background="@mipmap/gd_poker_bg_small"
                android:gravity="center"
                android:textColor="@color/red"
                android:textSize="27px"
                android:visibility="gone"
                app:layout_auto_basewidth="height" />

            <TextView
                android:id="@+id/gd__tv_bottom_panel_left_b"
                android:layout_width="32px"
                android:layout_height="32px"
                android:layout_gravity="left"
                android:layout_marginTop="100px"
                android:background="@drawable/gd_oval_blue_bg"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="17px"
                android:visibility="gone"
                app:layout_auto_basewidth="height" />

            <TextView
                android:id="@+id/gd__tv_bottom_panel_right_b"
                android:layout_width="32px"
                android:layout_height="32px"
                android:layout_gravity="right"
                android:layout_marginTop="100px"
                android:background="@drawable/gd_oval_red_bg"
                android:gravity="center"
                android:padding="3dp"
                android:textColor="@color/white"
                android:textSize="17px"
                android:visibility="gone"
                app:layout_auto_basewidth="height" />
        </FrameLayout>

        <LinearLayout
            android:id="@+id/gd__ll_poker_pw"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:clickable="false"
            android:orientation="horizontal"
            android:visibility="visible">

            <FrameLayout
                android:id="@+id/gd__fl_player_pw_parent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="3dp"
                android:gravity="center_vertical">

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="left|bottom"
                    android:layout_marginLeft="110px">

                    <ImageView
                        android:id="@+id/gd__img_left_player_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_player1"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:visibility="visible" />

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_player2"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:visibility="visible" />

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_player3"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:visibility="visible" />

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="right|bottom"
                    android:layout_marginRight="110px">

                    <ImageView
                        android:id="@+id/gd__img_right_player_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="right|bottom"
                    android:layout_marginRight="110px">

                    <ImageView
                        android:id="@+id/gd__img_center_player_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>

            </FrameLayout>


            <FrameLayout
                android:id="@+id/gd__fl_banker_pw_parent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="3dp"
                android:clickable="false"

                android:gravity="center_vertical">

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="left|bottom"
                    android:layout_marginLeft="110px">

                    <ImageView
                        android:id="@+id/gd__img_left_banker_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_banker1"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="3dp"
                    android:visibility="visible" />

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_banker2"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:visibility="visible" />

                <gaming178.com.casinogame.Control.PageWidgetT
                    android:id="@+id/gd__page_widget_poker_banker3"
                    android:layout_width="560px"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:visibility="visible" />

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="right|bottom"
                    android:layout_marginRight="110px">

                    <ImageView
                        android:id="@+id/gd__img_right_banker_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="30dp"
                    android:layout_height="260px"
                    android:layout_gravity="right|bottom"
                    android:layout_marginRight="110px">

                    <ImageView
                        android:id="@+id/gd__img_center_banker_rotate"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="left|center_vertical"
                        android:src="@mipmap/gd_img_rotate"
                        android:visibility="gone" />
                </FrameLayout>
            </FrameLayout>

        </LinearLayout>

    </FrameLayout>

</FrameLayout>