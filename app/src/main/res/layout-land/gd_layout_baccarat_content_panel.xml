<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="760px"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/gd__ll_baccarat_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/gd__baccarat_grid_parent_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">

                <HorizontalScrollView
                    android:id="@+id/gd__layout1"
                    android:layout_width="142dp"
                    android:layout_height="wrap_content"
                    android:scrollbars="none">

                    <include layout="@layout/gd_include_baccarat_grid_layout1" />
                </HorizontalScrollView>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/gd__ll_big_road_parent2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <HorizontalScrollView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:scrollbars="none">

                            <include layout="@layout/gd_include_baccarat_grid_layout2" />
                        </HorizontalScrollView>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/gd__ll_small_road_parent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-1dp"
                        android:orientation="horizontal">

                        <HorizontalScrollView
                            android:id="@+id/gd__hsv_small_road_1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:scrollbars="none">

                            <include layout="@layout/gd_include_baccarat_grid_layout3" />
                        </HorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />

                        <HorizontalScrollView
                            android:id="@+id/gd__hsv_small_road_2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:layout_weight="1"
                            android:scrollbars="none">

                            <include
                                android:id="@+id/gd__baccarat_gridlayout4"
                                layout="@layout/gd_include_baccarat_grid_layout3" />
                        </HorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />

                        <HorizontalScrollView
                            android:id="@+id/gd__hsv_small_road_3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:layout_weight="1"
                            android:scrollbars="none">

                            <include
                                android:id="@+id/gd__baccarat_gridlayout5"
                                layout="@layout/gd_include_baccarat_grid_layout3" />
                        </HorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="30px"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/gd__ll_banker_ask"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/red"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/gd__tv_ask1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:ems="1"
                            android:gravity="center"
                            android:text="@string/banker1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__tv_ask1_name"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:ems="1"
                            android:gravity="center"
                            android:text="@string/ask"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="14px"
                        android:src="@mipmap/gd_banker_ask"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_player_ask"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="1px"
                    android:layout_weight="1"
                    android:background="@color/blue_height"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/gd__tv_ask2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:ems="1"
                            android:gravity="center"
                            android:text="@string/player1"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/gd__tv_ask2_name"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:ems="1"
                            android:gravity="center"
                            android:text="@string/ask"
                            android:textColor="@color/white"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="14px"
                        android:src="@mipmap/gd_player_ask"></ImageView>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:layout_gravity="center"
            android:background="@color/white_grey_bg_dark"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">

            <TextView
                android:id="@+id/gd__text_shoe_game_number"
                style="@style/text_wrap_black12"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:minWidth="60dp"
                android:paddingRight="5dp"
                android:text="0-0"
                android:textColor="@color/black_grey" />

            <LinearLayout

                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/grey_thick_line"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/gd_rectangle_b_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_banker"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/banker1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_banker"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/gd_rectangle_p_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_player"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/player1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_player"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/gd_rectangle_tie_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_tie"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/tie1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_tie"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_bp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/gd_rectangle_bp_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_banker_pair"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/BP1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_bp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_pp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/gd_rectangle_pp_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_player_pair"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/PP1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_pp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/gd_rectangle_total_shape"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_total"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/total_m"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/gd__rl_good_road"
                android:layout_width="65dp"
                android:layout_height="match_parent"
                android:background="@mipmap/gd_good_road"
                android:paddingLeft="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_centerVertical="true"
                        android:src="@mipmap/gd_good" />


                    <TextView
                        android:id="@+id/gd__tv_good_road_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:text="@string/good_road"
                        android:textColor="@color/black"
                        android:textSize="9sp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/gd__tv_good_road_count"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:background="@drawable/gd_circle_b_shape"
                    android:gravity="center"
                    android:text="9"
                    android:textColor="@color/white"
                    android:textSize="8sp"
                    android:visibility="gone" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/gd__tv_banker_road"
                    style="@style/text_match_blue12"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="B"
                    android:textColor="@color/red" />

                <TextView
                    android:layout_width="8dp"
                    android:layout_height="8dp"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_oval_red_solid_not"
                    android:gravity="center" />

                <TextView
                    style="@style/text_match_blue12"
                    android:layout_marginLeft="3dp"
                    android:gravity="center"
                    android:text="/"
                    android:textColor="@color/red_title"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/gd_oval_red_solid"
                    android:gravity="center" />


                <TextView
                    android:id="@+id/gd__tv_player_road"
                    style="@style/text_match_blue12"
                    android:layout_marginLeft="15dp"
                    android:gravity="center"
                    android:text="P"
                    android:textColor="@color/blue" />

                <TextView
                    android:layout_width="8dp"
                    android:layout_height="8dp"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_oval_blue_solid_not"
                    android:gravity="center" />

                <TextView
                    style="@style/text_match_blue12"
                    android:layout_marginLeft="3dp"
                    android:gravity="center"
                    android:text="/"
                    android:textColor="@color/blue"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/gd_oval_blue_solid"
                    android:gravity="center"
                    android:text=" " />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>