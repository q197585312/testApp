<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <!--COLOK MACAU -->
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#C1C1C1"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:id="@+id/colok_header_tv11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="5dp"
                android:text="COLOK MACAU"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/colok_header_tv12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />
            <TextView
                android:id="@+id/colok_header_key_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />
            <TextView
                android:id="@+id/colok_header_tv13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />
            <TextView
                android:id="@+id/colok_header_tv14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="5dp"
            android:textColor="@color/black"
            android:textSize="12sp" />

            <TextView
                android:id="@+id/colok_header_tv15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />
            <TextView
                android:id="@+id/colok_header_tv16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="5dp"
            android:textColor="@color/black"
            android:textSize="12sp" />



        </LinearLayout>
<View
   style="@style/line_vertical_grey1"
    android:layout_width="5dp"
    android:background="@color/grey_default_bg"/>
        <!-- COLOK NAGA-->
        <LinearLayout
            android:id="@+id/colok_header_ll2"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#C1C1C1"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:id="@+id/colok_header_tv21"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="COLOK NAGA"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/colok_header_tv22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/colok_header_tv23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:paddingLeft="5dp"
                android:textColor="@color/black"
                android:textSize="12sp" />

        </LinearLayout>
    </LinearLayout>
    <!-- clock 号码-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"

        >

        <TextView
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/colok_number"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/colok_header_location_tv"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="2.5"
            android:gravity="center"
            android:text="@string/weizhi"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:visibility="gone" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/amount_lottery"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/net_amount"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/colok_header_number_title_tv2"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/colok_number"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/colok_header_money_title_tv2"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/amount_lottery"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/colok_header_count_title_tv2"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/net_amount"
            android:textColor="@color/black" />
    </LinearLayout>
    <!-- 1-->
    <LinearLayout
        android:id="@+id/colok_content_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="5dp">

    </LinearLayout>
</LinearLayout>