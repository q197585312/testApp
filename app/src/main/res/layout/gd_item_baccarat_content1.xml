<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:descendantFocusability="blocksDescendants"
    android:focusable="true"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/gd__ll_baccarat_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/gd_way_bg"
        android:padding="2.5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:background="@drawable/gd_rectangle_brown_top_corner5"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/gd__tv_baccarat_table_name"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="2dp"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:padding="2dp"
                    android:text="BACCARAT01"
                    android:textColor="@color/white"
                    android:textSize="10sp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/gd__text_shoe_game_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:text="0-0"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:visibility="visible" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/gd_rectangle_b_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_banker"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/banker1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_banker"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_rectangle_p_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_player"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/player1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_player"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_rectangle_tie_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:id="@+id/gd__tv_tie"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/tie1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_tie"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_bp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_rectangle_bp_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/gd__tv_banker_pair"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/BP1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_bp"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_pp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_rectangle_pp_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/gd__tv_player_pair"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/PP1"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_pp"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:background="@drawable/gd_rectangle_total_shape"
                    android:orientation="horizontal"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/total_m"
                        android:textColor="@color/white"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/gd__text_total"
                        style="@style/text_wrap_black12"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/gd__ll_more_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                </LinearLayout>

                <TextView
                    android:id="@+id/gd__baccarat_status_tv"
                    android:layout_width="24dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/gd_rectangle_trans_stroke_white"
                    android:gravity="center"
                    android:minWidth="100dp"
                    android:padding="1dp"
                    android:singleLine="true"
                    android:text="0"
                    android:textColor="@color/yellow_brown_a3721a"
                    android:textSize="14sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gd_rectangle_white_bottom_corner5"
                android:orientation="horizontal">


                <LinearLayout
                    android:id="@+id/gd__ll_layout_girl"
                    android:layout_width="85dp"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="18dp"
                        android:layout_gravity="center_horizontal|bottom"
                        android:layout_marginTop="4dp"
                        android:background="@mipmap/gd_strip_tags_orange"
                        android:gravity="center_vertical"
                        android:padding="3dp"
                        android:text="@string/gd_enter"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        android:textStyle="bold" />

                    <FrameLayout

                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:background="@color/white">

                        <TextView
                            android:id="@+id/gd__tv_baccarat_dealer_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|center_horizontal"
                            android:gravity="center"
                            android:padding="2dp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/white"
                            android:textSize="9sp"
                            android:textStyle="bold"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/gd__baccarat_head_iv"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:scaleType="centerCrop" />
                    </FrameLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/brown_dark_bg" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="3dp"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/gd__baccarat_grid_parent_ll"
                        android:layout_width="match_parent"

                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal">

                        <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                            android:id="@+id/gd__layout1"
                            android:layout_width="113dp"
                            android:layout_height="wrap_content"
                            android:focusable="false"
                            android:scrollbars="none">

                            <include layout="@layout/gd_include_baccarat_grid_layout1" />
                        </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:clickable="false"
                                android:scrollbars="none">

                                <include
                                    android:id="@+id/gd__big_way"
                                    layout="@layout/gd_include_baccarat_grid_layout2" />
                            </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/gd__tv_banker_road"
                                style="@style/text_match_blue12"
                                android:gravity="center"
                                android:text="B"
                                android:textColor="@color/red_title" />

                            <TextView

                                android:layout_width="8dp"
                                android:layout_height="8dp"
                                android:background="@drawable/gd_oval_red_solid_not"
                                android:gravity="center" />

                            <TextView
                                style="@style/text_match_blue12"
                                android:gravity="center"
                                android:text="/"
                                android:textColor="@color/blue"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="6dp"
                                android:layout_height="6dp"
                                android:layout_marginTop="2dp"
                                android:background="@drawable/gd_oval_red_solid"
                                android:gravity="center" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/gd__tv_player_road"
                                style="@style/text_match_blue12"
                                android:gravity="center"
                                android:text="P"
                                android:textColor="@color/blue" />

                            <TextView

                                android:layout_width="8dp"
                                android:layout_height="8dp"
                                android:background="@drawable/gd_oval_blue_solid_not"
                                android:gravity="center" />

                            <TextView
                                style="@style/text_match_blue12"
                                android:gravity="center"
                                android:text="/"
                                android:textColor="@color/red_title"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="6dp"
                                android:layout_height="6dp"
                                android:layout_marginTop="2dp"
                                android:background="@drawable/gd_oval_blue_solid"
                                android:gravity="center"
                                android:text=" " />

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal">

                        <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:clickable="false"
                            android:scrollbars="none">

                            <include layout="@layout/gd_include_baccarat_grid_layout3" />
                        </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />

                        <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:layout_weight="1"
                            android:clickable="false"
                            android:scrollbars="none">

                            <include
                                android:id="@+id/gd__baccarat_gridlayout4"
                                layout="@layout/gd_include_baccarat_grid_layout3" />
                        </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />

                        <gaming178.com.casinogame.Control.ParentClickHorizontalScrollView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="1dp"
                            android:layout_weight="1"
                            android:clickable="false"
                            android:scrollbars="none">

                            <include
                                android:id="@+id/gd__baccarat_gridlayout5"
                                layout="@layout/gd_include_baccarat_grid_layout3" />
                        </gaming178.com.casinogame.Control.ParentClickHorizontalScrollView>

                        <View style="@style/line_vertical_grey1" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <include layout="@layout/gd_good_road_bg"></include>

        <include layout="@layout/gd_table_close_bg"></include>

        <include layout="@layout/gd_shuffling" />

        <include
            layout="@layout/gd_popupwindow_limit_select_layout"
            android:visibility="gone" />
    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"></View>
</LinearLayout>