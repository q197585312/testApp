<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:keepScreenOn="true"
    android:orientation="vertical" >


    <include layout="@layout/toolbar_layout"

        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dip"
            android:background="@drawable/title_no_conor_shape"

            >
            <ImageView
                android:id="@+id/back"
                android:layout_width="80dip"
                android:layout_height="60dip"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@drawable/v1_player_med_back"
                />
            <TextView
                android:id="@+id/tv_titletop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="hhh"
                android:textColor="@color/blue"
                android:textSize="20dip"
                />
        </RelativeLayout>
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="false">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200dip"
                android:orientation="horizontal"
                android:weightSum="3"
                >
                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="200dip"
                    android:layout_weight="0.7"
                    android:orientation="vertical"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/dig_no_conor_big_shape"
                        android:orientation="vertical" >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:id="@+id/rl1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingTop="10dp"
                                >
                                <TextView
                                    android:id="@+id/tv_time"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:gravity="center"
                                    android:text="00:00"
                                    android:textColor="#Ffffff"
                                    android:textSize="30dip"
                                    android:textStyle="bold" />

                            </RelativeLayout>
                            <RelativeLayout
                                android:id="@+id/rl2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/rl1"
                                android:layout_marginTop="25dip"
                                android:gravity="center"
                                >
                                <Button
                                    android:id="@+id/btn_login"
                                    android:layout_width="45dip"
                                    android:layout_height="45dip"
                                    android:layout_marginLeft="20dip"
                                    android:background="@drawable/toback"
                                    android:gravity="center"
                                    android:textColor="#da603c"
                                    />
                                <ImageView
                                    android:id="@+id/refresh_list"
                                    android:layout_width="45dip"
                                    android:layout_height="45dip"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginLeft="@dimen/dimen_5dp"
                                    android:layout_marginRight="20dip"
                                    android:src="@drawable/refresh_numlist"
                                    />

                            </RelativeLayout>
                            <TextView
                                android:id="@+id/tv_cycle"
                                android:layout_width="match_parent"
                                android:layout_height="25dip"
                                android:layout_alignParentBottom="true"
                                android:layout_marginBottom="5dip"
                                android:background="@drawable/dig_no_conor_orange_shape"
                                android:gravity="center"
                                android:text="20151026130"
                                android:textColor="#666666"
                                />

                        </RelativeLayout>


                    </LinearLayout>
                </LinearLayout>

                <!-- 直播视频-->
                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="200dip"
                    android:layout_weight="1.3"
                    android:background="@drawable/dig_no_conor_redhall_shape"
                    android:orientation="vertical">
                    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center">
                        <FrameLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent">
                            <nanyang.com.dig88.Ijkplayer.IjkVideoView
                                android:id="@+id/b_imageView_video"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:scaleType="fitXY"
                                >
                            </nanyang.com.dig88.Ijkplayer.IjkVideoView>

                            <Button
                                android:id="@+id/b_btn_video_reload"
                                android:layout_width="40dip"
                                android:layout_height="40dip"
                                android:layout_gravity="right|bottom"
                                android:layout_marginRight="10dip"
                                android:layout_marginBottom="5dip"
                                android:background="@drawable/stop_video"
                                android:onClick="OnClickVideo" />

                            <TextView
                                android:id="@+id/b_text_video"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                 android:layout_gravity="center"
                                android:text="Load....."
                                android:textColor="#FFFF"
                                android:textSize="20sp"
                                android:typeface="serif"
                                android:visibility="gone" />

                            <Button
                                android:id="@+id/b_voice"
                                android:layout_width="70dip"
                                android:layout_height="50dip"
                                android:layout_gravity="right|bottom"
                                android:layout_marginRight="90dip"
                                android:layout_marginBottom="10dip"
                                android:background="@drawable/btn_voice"
                                android:onClick="OnClickVoice"
                                android:visibility="gone" />
                            <TextView
                                android:id="@+id/videoErrorMesseg"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Video Delay please check you network Please click the play button to reconnect"
                                android:textColor="#fae204"
                                android:textSize="20sp"
                                android:typeface="serif"
                                android:visibility="gone" />
                        </FrameLayout>
                    </RelativeLayout>

                </LinearLayout>

                <!-- 最右边的结果列表-->
                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="200dip"
                    android:layout_weight="1"
                    >
                    <!-- 球的-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:orientation="vertical"
                        >

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dip"
                            android:background="#f1f1f1"
                            android:gravity="center"
                            >
                            <TextView
                                android:id="@+id/tv_ball_result"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:paddingRight="20dip"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                />
                            <TextView
                                android:id="@+id/tv_more"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:text="@string/gengduo"
                                android:textColor="@color/black"
                                android:textSize="15dp"
                                />

                        </RelativeLayout>
                        <ListView
                            android:id="@+id/lv_ball_result"
                            android:layout_width="match_parent"
                            android:layout_height="200dip"
                            android:background="@color/white"
                            android:divider="@null"
                            >
                        </ListView>
                        <TextView
                            android:id="@+id/tv_nodata"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="50dip"
                            android:gravity="center"
                            android:text="@string/nadata"
                            android:visibility="gone"
                            />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        <FrameLayout
            android:id="@+id/fragment_game_hall_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <!-- 号码   大  小  双   单   全部  清楚  投注-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_gravity="center"
                android:orientation="vertical"

                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#333333"

                    >
                    <TextView
                        android:id="@+id/tv_bet_pop_odds"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingLeft="5dip"
                        android:textColor="@color/white"
                        android:textSize="17dp"

                        />

                    <LinearLayout
                        android:id="@+id/layout_sort0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_big"
                            android:layout_width="50dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:text="@string/big"
                            android:textColor="@color/white"
                            />
                        <Button
                            android:id="@+id/btn_small"
                            android:layout_width="60dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:gravity="center"
                            android:text="@string/small"
                            android:textColor="@color/white"
                            />
                        <Button
                            android:id="@+id/btn_double"
                            android:layout_width="60dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:text="@string/even_lottery"
                            android:textColor="@color/white"
                            />
                        <Button
                            android:id="@+id/btn_simgle"
                            android:layout_width="60dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:text="@string/odd_lottery"
                            android:textColor="@color/white"
                            />
                        <Button
                            android:id="@+id/tv_all"
                            android:layout_width="60dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:text="@string/all"
                            android:textColor="@color/white"
                            android:textSize="14dip"
                            />
                        <Button
                            android:id="@+id/tv_delete"
                            android:layout_width="60dip"
                            android:layout_height="match_parent"
                            android:background="#333333"
                            android:gravity="center"
                            android:text="@string/qingchu"
                            android:textColor="@color/white"


                            />
                        <Button
                            android:id="@+id/tv_bet_pop_number"
                            android:layout_width="60dip"
                            android:layout_height="wrap_content"
                            android:background="#333333"
                            android:gravity="center"
                            android:text="@string/touzhu"
                            android:textColor="@color/white"

                            />
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/layout_sort0"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dip"
                        >

                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>


            <!-- 1-42数字-->
            <LinearLayout
                android:id="@+id/number_parent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#4c4c4c"
                android:orientation="vertical">

            </LinearLayout>

            <!-- 下面 大 小 双 单 红 黄 蓝 start-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="115dip"
                android:layout_marginLeft="5dip"
                android:layout_marginTop="5dip"
                android:layout_marginRight="5dip"
                android:background="#000000"
                android:orientation="horizontal"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1.25"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="3dip"
                        android:layout_weight="0.7"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/rl_big"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.2"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/big"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_big_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_small"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.2"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/small"

                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_small_odds"
                                android:layout_width="wrap_content"

                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_double"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.2"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/even_lottery"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_double_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_single"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.2"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/odd_lottery"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_single_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />
                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="1dip"
                        android:layout_weight="0.7">
                        <LinearLayout
                            android:id="@+id/rl_small_double"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1875"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/small_even"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_small_double_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/tv5"
                                android:text="3.6"
                                android:textColor="#ffad14"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_small_single"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.1875"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/small_odd"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_small_single_odds"
                                android:layout_width="wrap_content"

                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_big_double"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.1875"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >
                            <TextView
                                android:id="@+id/tv8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/big_even"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_big_double_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1.9"
                                android:textColor="#ffad14"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_big_single"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="3dip"
                            android:layout_weight="0.1875"
                            android:background="@drawable/dig_no_conor_light_orange_shape"
                            android:gravity="center"
                            android:orientation="vertical"
                            >

                            <TextView
                                android:id="@+id/tv7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:paddingBottom="5dip"
                                android:singleLine="true"
                                android:text="@string/big_odd"
                                android:textColor="@color/white"
                                />
                            <TextView
                                android:id="@+id/tv_big_single_odds"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="1.9"

                                android:textColor="#ffad14"
                                />
                        </LinearLayout>



                    </LinearLayout>


                </LinearLayout>



                <LinearLayout
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1.75"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/rl_red"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:background="@drawable/dig_no_conor_light_orange_shape"
                        android:gravity="center"
                        >
                        <TextView
                            android:id="@+id/red"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:padding="5dip"
                            android:text="@string/hong"
                            android:textColor="@color/white"
                            android:textSize="17dip"
                            android:textStyle="bold"

                            />

                        <TextView
                            android:id="@+id/tv_red"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="100dip"
                            android:layout_marginTop="5dip"
                            android:layout_toRightOf="@+id/red"
                            android:text="01,04, 07, 10, 13, 16, 19\n22, 25, 28, 31, 34, 37, 40"
                            android:textColor="#666666"
                            android:textSize="10dip"
                            android:textStyle="bold"
                            />

                        <TextView
                            android:id="@+id/tv_red_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="5dip"
                            android:padding="5dip"
                            android:text=""
                            android:textColor="#ffad14"
                            android:textSize="11dip"


                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_yellow"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dip"
                        android:layout_weight="0.5"
                        android:background="@drawable/dig_no_conor_light_orange_shape"
                        android:gravity="center"
                        >
                        <TextView
                            android:id="@+id/yellow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:padding="5dip"
                            android:text="@string/huang"
                            android:textColor="@color/white"
                            android:textSize="17dip"
                            android:textStyle="bold"
                            />
                        <TextView
                            android:id="@+id/tv_yellow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="100dip"
                            android:layout_marginTop="5dip"
                            android:layout_toRightOf="@+id/yellow"
                            android:text="02,05, 08, 11, 14, 17, 20,\n23, 26, 29, 32, 35, 38, 41"
                            android:textColor="#666666"
                            android:textSize="10dip"
                            android:textStyle="bold"
                            />
                        <TextView
                            android:id="@+id/tv_yelllow_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="5dip"
                            android:padding="5dip"
                            android:text=""
                            android:textColor="#ffad14"
                            android:textSize="11dip"


                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_blue"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="2dip"
                        android:layout_weight="0.5"
                        android:background="@drawable/dig_no_conor_light_orange_shape"
                        android:gravity="center"
                        >
                        <TextView
                            android:id="@+id/blue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:padding="5dip"
                            android:text="@string/lan"
                            android:textColor="@color/white"
                            android:textSize="17dip"
                            android:textStyle="bold"

                            />
                        <TextView
