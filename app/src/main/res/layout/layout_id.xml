<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/ll_register_bg"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/gd_login_button_bg_white2"
        android:gravity="center"
        android:orientation="horizontal"></LinearLayout>

    <ImageView
        android:id="@+id/img_ula_enter_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></ImageView>

    <FrameLayout
        android:id="@+id/fl_whatsapp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></FrameLayout>

    <TextView
        android:id="@+id/gd__tv_live_chat"
        android:layout_width="match_parent"
        android:layout_height="40dp" />

    <ImageView
        android:id="@+id/img_whatsapp"
        android:layout_width="match_parent"
        android:layout_height="45dp"></ImageView>

    <ImageView
        android:id="@+id/img_fb"
        android:layout_width="match_parent"
        android:layout_height="45dp"></ImageView>

    <RelativeLayout
        android:id="@+id/rl_live_chat"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"></RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_whatsapp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"></RelativeLayout>

    <gaming178.com.casinogame.Control.AutoScrollTextView
        android:id="@+id/gd__hall_game_bottom_prompt_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rc_raja"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"></androidx.recyclerview.widget.RecyclerView>

    <FrameLayout
        android:id="@+id/fl_slide_left"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="center"
        android:visibility="gone"></FrameLayout>

    <FrameLayout
        android:id="@+id/fl_slide_right"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_gravity="center"
        android:visibility="gone"></FrameLayout>

    <ImageView
        android:id="@+id/img_slide2"
        android:layout_width="220dp"
        android:layout_height="189dp"
        android:layout_marginLeft="-200dp"
        android:layout_marginTop="320dp"
        android:visibility="gone"></ImageView>

    <ImageView
        android:id="@+id/img_slide1"
        android:layout_width="27dp"
        android:layout_height="189dp"
        android:layout_marginTop="320dp"
        android:visibility="gone"></ImageView>

    <FrameLayout
        android:id="@+id/fl_jackpot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"></FrameLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rc_raja2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"></androidx.recyclerview.widget.RecyclerView>

    <LinearLayout
        android:id="@+id/ll_animation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:visibility="gone"></LinearLayout>

    <TextView
        android:id="@+id/gd_tv_count"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:visibility="gone"></TextView>

    <LinearLayout
        android:id="@+id/ll_pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_lc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ahl_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_login_prent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_login_prent_exit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone"></LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_name_out_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"></FrameLayout>

    <FrameLayout
        android:id="@+id/fl_password_out_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"></FrameLayout>

    <LinearLayout
        android:id="@+id/ll_ahl_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_wa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_tg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_liveChat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="horizontal"></LinearLayout>

    <ImageView
        android:id="@+id/img_animation1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ImageView>

    <ImageView
        android:id="@+id/img_animation2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ImageView>

    <ImageView
        android:id="@+id/img_animation3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ImageView>

    <ImageView
        android:id="@+id/img_animation4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ImageView>

    <LinearLayout
        android:id="@+id/ll_banner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone"></LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ahl_game_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone"></LinearLayout>

    <RecyclerView
        android:id="@+id/rc_liveCasino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></RecyclerView>

    <RecyclerView
        android:id="@+id/rc_sportBook_JudiBola"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></RecyclerView>

    <RecyclerView
        android:id="@+id/rc_slotOnline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></RecyclerView>

    <RecyclerView
        android:id="@+id/rc_pokerOnline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></RecyclerView>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></androidx.core.widget.NestedScrollView>

    <LinearLayout
        android:id="@+id/ahl_ll_user1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone"></LinearLayout>

    <LinearLayout
        android:id="@+id/ahl_ll_user2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone"></LinearLayout>

    <TextView
        android:id="@+id/ahl_tv_home_user_name"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/ahl_lobby_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/gd_arrow_left_back"
        android:visibility="gone"></ImageView>

    <LinearLayout
        android:id="@+id/ll_d_w"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="gone">

        <ImageView
            android:id="@+id/img_d"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"></ImageView>

        <ImageView
            android:id="@+id/img_w"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"></ImageView>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_rtp"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:drawableTop="@mipmap/gd_home_null"
            android:gravity="center"
            android:paddingTop="10dp"
            android:text="Bocoran"
            android:textColor="@color/white"
            android:textSize="13sp"></TextView>

        <ImageView
            android:id="@+id/img_rtp"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="8dp"></ImageView>
    </FrameLayout>
</FrameLayout>