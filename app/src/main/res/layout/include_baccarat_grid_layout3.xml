<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fl_small_road_parent"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="false">

    <gaming178.com.mylibrary.myview.View.GridBackgroundView
        android:id="@+id/smallway_item"
        android:layout_width="241dp"
        android:layout_height="37dp"
        android:clickable="false"
        android:focusable="false"
        android:visibility="visible"
        app:dividing_line_spacing="6dp"
        app:line_color="@color/grey_default_bg" />

    <gaming178.com.mylibrary.myview.View.GridBackgroundView
        android:id="@+id/smallway_item_big"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:clickable="false"
        android:focusable="false"
        android:visibility="gone"
        app:dividing_line_spacing="20dp"
        app:line_color="@color/grey_default_bg" />

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/baccarat_gridlayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="false"
        android:columnCount="40"
        android:focusable="false"
        android:orientation="vertical"
        android:rowCount="6"></GridLayout>
</FrameLayout>