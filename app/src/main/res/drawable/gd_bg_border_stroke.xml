<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="1.5dp"
                android:color="#E8A227"
                android:dashWidth="15dp"
                android:dashGap="5dp" />

            <solid android:color="#00FFFFFF" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>