<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_lobby_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/gd_home_bottom"
    android:orientation="vertical">

    <include
        layout="@layout/toolbar_layout"
        android:visibility="gone" />

    <LinearLayout
        android:id="@+id/ll_top_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#70000000"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/gd__ll_notice"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/gd_notice_home" />

                <gaming178.com.casinogame.Control.AutoScrollTextView
                    android:id="@+id/gd__hall_game_bottom_prompt_tv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical" />

            </LinearLayout>

            <TextView
                android:id="@+id/tv_lg"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="2dp"
                android:drawableLeft="@mipmap/gd_lang_en_flag"
                android:drawablePadding="3dp"
                android:gravity="center"
                android:text="@string/language"
                android:textAllCaps="true"
                android:textColor="@color/white"></TextView>

            <ImageView
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="10dp"
                android:src="@mipmap/white_bottom"></ImageView>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/gd_img_logo"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="6dp"
                android:layout_marginBottom="6dp"
                android:adjustViewBounds="true"
                android:maxWidth="100dp"
                android:src="@mipmap/gd_title_logo" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"></View>

            <TextView
                android:id="@+id/tv_set"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/shape_home_black"
                android:drawableLeft="@mipmap/home_set"
                android:drawablePadding="4dp"
                android:paddingLeft="6dp"
                android:paddingTop="3dp"
                android:paddingRight="6dp"
                android:paddingBottom="3dp"
                android:text="@string/setting"
                android:textAllCaps="true"
                android:textColor="@color/white"
                android:visibility="gone"></TextView>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/gd_name_bg"
                android:orientation="horizontal"
                android:paddingLeft="20dp"
                android:paddingTop="8dp"
                android:paddingRight="20dp"
                android:paddingBottom="8dp">

                <TextView
                    android:id="@+id/gd__tv_home_balance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:drawableLeft="@mipmap/home_money"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:text="xxx"
                    android:textColor="@color/white"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/gd__tv_home_user_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:drawableLeft="@mipmap/home_user"
                    android:drawablePadding="3dp"
                    android:gravity="center"
                    android:text="xxx"
                    android:textColor="@color/white"
                    android:textSize="15sp" />


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_switch_account"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:background="@mipmap/home_yellow_button"
                android:gravity="center"
                android:padding="6dp"
                android:text="@string/logout"
                android:textAllCaps="true"
                android:textColor="@color/home_select_color"
                android:visibility="gone"></TextView>

            <LinearLayout
                android:id="@+id/gd_ll_menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/shape_home_black"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="12dp"
                android:paddingTop="5dp"
                android:paddingRight="12dp"
                android:paddingBottom="5dp"
                android:visibility="gone">

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@mipmap/lobby_menu"></ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:text="@string/gd_Menu"
                    android:textColor="@color/white"
                    android:textSize="15sp"></TextView>
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rl_banner"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:visibility="gone">

        <gaming178.com.casinogame.Util.BannerViewPager
            android:id="@+id/auto_viewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <LinearLayout
            android:id="@+id/in_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_d_w"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        android:paddingLeft="6dp"
        android:paddingRight="6dp"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/main_tv_home_user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@mipmap/home_user"
                android:drawablePadding="3dp"
                android:text="xxx"
                android:textColor="@color/white"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/main_tv_home_balance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:drawableLeft="@mipmap/home_money"
                android:drawablePadding="3dp"
                android:text="xxx"
                android:textColor="@color/home_no_select_color"
                android:textSize="15sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_d"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"></ImageView>

            <ImageView
                android:id="@+id/img_w"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"></ImageView>
        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/view_game_parent"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/fl_my_game"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="8dp"
            android:paddingTop="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="horizontal"
                android:paddingRight="8dp">

                <View
                    android:id="@+id/view_item"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />


                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="8dp"
                    android:layout_weight="1" />

            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/gd__gridview_content_gv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"></androidx.recyclerview.widget.RecyclerView>

        </FrameLayout>

        <LinearLayout
            android:id="@+id/ll_parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:layout_marginTop="8dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_content_bg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#039B00"
                android:orientation="vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/tv_home_close_game"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="12dp"
                    android:drawableRight="@mipmap/xx"
                    android:gravity="center"
                    android:text="@string/baccarat"
                    android:textAllCaps="true"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:visibility="gone"></TextView>

                <FrameLayout
                    android:id="@+id/fl_home"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="15dp"></FrameLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_bottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:background="#50000000"
            android:orientation="vertical">

            <View
                android:id="@+id/view_top"
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:visibility="gone"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <View
                    android:id="@+id/view_1"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

                <TextView
                    android:id="@+id/tv_home_home"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:drawableTop="@mipmap/gd_home_home"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="@string/gd_home"
                    android:textColor="@color/white"
                    android:textSize="13sp"></TextView>

                <View
                    android:id="@+id/view_2"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

                <TextView
                    android:id="@+id/tv_home_deposit"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:drawableTop="@mipmap/gd_home_deposit"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="@string/gd_deposit"
                    android:textColor="@color/white"
                    android:textSize="13sp"></TextView>

                <FrameLayout
                    android:id="@+id/fl_rtp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawableTop="@mipmap/gd_home_null"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:text="Bocoran"
                        android:textColor="@color/white"
                        android:textSize="13sp"></TextView>

                    <ImageView
                        android:id="@+id/img_rtp"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="8dp"></ImageView>
                </FrameLayout>

                <View
                    android:id="@+id/view_3"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

                <TextView
                    android:id="@+id/tv_home_withdraw"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:drawableTop="@mipmap/gd_home_withdraw"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="@string/gd_withdrawal"
                    android:textColor="@color/white"
                    android:textSize="13sp"></TextView>

                <FrameLayout
                    android:id="@+id/fl_whatsapp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawableTop="@mipmap/gd_home_null"
                        android:gravity="center"
                        android:paddingTop="10dp"
                        android:text="Whatsapp"
                        android:textColor="@color/white"
                        android:textSize="13sp"></TextView>

                    <ImageView
                        android:id="@+id/img_whatsapp"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="8dp"></ImageView>
                </FrameLayout>


                <View
                    android:id="@+id/view_4"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

                <TextView
                    android:id="@+id/tv_home_live_chat"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:drawableTop="@mipmap/gd_home_live_chat"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="Livechat"
                    android:textColor="@color/white"
                    android:textSize="13sp"
                    android:visibility="gone"></TextView>

                <View
                    android:id="@+id/view_5"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

                <TextView
                    android:id="@+id/tv_home_logout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:drawableTop="@mipmap/gd_home_exit"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="@string/logout"
                    android:textColor="@color/white"
                    android:textSize="13sp"></TextView>

                <View
                    android:id="@+id/view_6"
                    android:layout_width="8dp"
                    android:layout_height="match_parent"
                    android:visibility="gone"></View>

            </LinearLayout>

            <View
                android:id="@+id/view_bottom"
                android:layout_width="match_parent"
                android:layout_height="8dp"
                android:visibility="gone"></View>


        </LinearLayout>

    </FrameLayout>

    <TextView
        android:id="@+id/tv_switch_account_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></TextView>

    <TextView
        android:id="@+id/tv_home_deposit_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></TextView>

    <TextView
        android:id="@+id/tv_home_withdraw_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"></TextView>
</LinearLayout>