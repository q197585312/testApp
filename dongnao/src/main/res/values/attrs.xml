<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="myProgress">
        <attr name="roundProgressColor" format="color"></attr>
        <attr name="roundColor" format="color"></attr>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="roundTextSize" format="dimension"></attr>
        <attr name="roundTextColor" format="color"></attr>
        <attr name="max" format="integer"></attr>
        <attr name="textShow" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>

        </attr>
    </declare-styleable>

</resources>