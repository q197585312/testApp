                        android:layout_height="wrap_content"
                        android:text="1,3,5,7,9"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="11sp" />
                </LinearLayout>
            </com.nanyang.app.Utils.ChildPressdRelativeLayout>


            <com.nanyang.app.Utils.ChildPressdRelativeLayout
                android:id="@+id/rl_set_bottom"
                android:layout_width="0dp"
                android:layout_height="match_parent"
