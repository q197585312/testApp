                        android:textColor="@color/keno_text_color2"
                        android:textSize="13sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1-40"
                        android:textColor="@color/keno_text_color1"
