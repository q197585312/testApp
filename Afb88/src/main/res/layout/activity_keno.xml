<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar_layout" />

    <RadioGroup
        android:id="@+id/rg_tab"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:background="@mipmap/keno_tab_bg"
        android:orientation="horizontal"
        android:paddingBottom="6dp"
        android:paddingTop="6dp">

        <RadioButton
            android:id="@+id/rb_china"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/keno_country_type_seleter"
            android:button="@null"
            android:checked="true"
            android:gravity="center"
            android:text="@string/china"
            android:textColor="@color/keno_tab_color"
            android:textSize="12sp" />

        <RadioButton
            android:id="@+id/rb_canada1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/keno_country_type_seleter"
            android:button="@null"
            android:gravity="center"
            android:text="@string/canada1"
            android:textColor="@color/keno_tab_color"
            android:textSize="12sp" />

        <RadioButton
            android:id="@+id/rb_canada2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/keno_country_type_seleter"
            android:button="@null"
            android:gravity="center"
            android:text="@string/canada2"
            android:textColor="@color/keno_tab_color"
            android:textSize="12sp" />

        <RadioButton
            android:id="@+id/rb_slovakia"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/keno_country_type_seleter"
            android:button="@null"
            android:gravity="center"
            android:text="@string/slovakia"
            android:textColor="@color/keno_tab_color"
            android:textSize="12sp" />

        <RadioButton
            android:id="@+id/rb_australia"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/keno_country_type_seleter"
            android:button="@null"
            android:gravity="center"
            android:text="@string/australia"
            android:textColor="@color/keno_tab_color"
            android:textSize="12sp" />
    </RadioGroup>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:background="@mipmap/keno_count_down_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="-18dp"
            android:layout_marginRight="-11dp">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="55dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="18dp"
                android:background="@mipmap/keno_title_name" />

            <com.nanyang.app.Utils.RotateTextView
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/website"
                android:textSize="11sp" />
        </RelativeLayout>

        <ImageView
            android:id="@+id/img_website"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:background="@mipmap/keno_china_logo" />

        <View
            android:layout_width="2px"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:background="@color/keno_line" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/current_draw"
                android:textSize="13sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="@color/keno_line" />

            <TextView
                android:id="@+id/tv_draw_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="0578496"
                android:textColor="@color/white"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_draw_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="11/11 01:31PM"
                android:textSize="10sp" />

        </LinearLayout>

        <View
            android:layout_width="2px"
            android:layout_height="match_parent"
            android:background="@color/keno_line" />

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2.5"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:text="@string/count_down"
                android:textColor="@color/black"
                android:textSize="10sp" />

            <TextView
                android:id="@+id/tv_count_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:background="@mipmap/keno_time_bg"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="12sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <View
            android:layout_width="2px"
            android:layout_height="match_parent"
            android:background="@color/keno_line" />

        <LinearLayout
            android:id="@+id/ll_result"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/img_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@mipmap/keno_result_white" />

            <TextView
                android:id="@+id/tv_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/result"
                android:textColor="@color/white"
                android:textSize="10sp" />
        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.3">

        <LinearLayout
            android:id="@+id/ll_bet"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/keno_grenn"
            android:orientation="vertical"
            android:padding="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_big"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/big_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_big_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1410-811"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>


                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_set_top"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/selector_keno_set_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="6dp"
                        android:text="@string/tie_up_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="13sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="6dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_tie_top_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="810"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_small"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/small_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_small_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="809-210"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_upper"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/up_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_up_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1-40"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>


                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_mid"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/selector_keno_set_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="6dp"
                        android:text="@string/mid_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="13sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="6dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_mid_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_lower"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/down_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_down_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="41-80"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_odd"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/odd_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_odd_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1,3,5,7,9"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>


                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_set_bottom"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1.3"
                    android:background="@drawable/selector_keno_set_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="6dp"
                        android:text="@string/tie_bottom_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="13sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="6dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_tie_bottom_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_even"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/even_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_even_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0,2,4,6,8"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_single"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/single_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_single_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1,3,5,7,9"
                            android:textColor="@color/keno_text_color1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </com.nanyang.app.Utils.ChildPressdRelativeLayout>


                <com.nanyang.app.Utils.ChildPressdRelativeLayout
                    android:id="@+id/rl_double"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="2"
                    android:background="@drawable/selector_keno_big_small_bet">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="@string/double_keno"
                        android:textColor="@color/keno_text_color1"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:gravity="center|right"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_double_odds"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/keno_text_color2"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0,2,4,6,8"
                            android:textColor="@color/keno_text_color1"
